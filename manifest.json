{
  "name": "Extension Manager",
  "version": "1.14.0",
  "manifest_version": 3,
  "description": "A simple tool to enable, disable, or remove your Chrome extensions with ease.",
  "icons" : {
    "16" : "images/icon16.png",
    "48" : "images/icon48.png",
    "128" : "images/icon128.png"
  },
  "action": {
    "default_icon": "images/iconbar.png",
    "default_title": "Extension Manager",
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "js/migration.js"
  },
  "permissions": ["management","storage"]
}
